/**
 * @see https://www.electron.build/configuration/configuration
 */
 {
  "productName": "My Epub Reader",
  "asar": true,
  "directories": {
    "output": "release/${version}"
  },
  "files": [
    "dist"
  ],
  "mac": {
    "artifactName": "${productName}.${ext}",
    "target": [
      "zip"
    ]
  },
  "win": {
    "artifactName": "${productName}.${ext}",
    "target": [
      {
        "target": "zip",
        "arch": [
          "x64"
        ]
      }
    ]
  }
}
